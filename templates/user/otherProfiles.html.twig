{% extends 'base.html.twig' %}

{% block Pagetitle %}
    Profil de {{ user.username }} :
{% endblock %}

{% block main %}

    <p>
        Prénom : {{ user.name }} <br>
        Nom : {{ user.surname }} <br>
        Email : {{ user.email }} <br>
